use Bot\BotCore;

/**
 * Composer autoloader
 */
error_reporting(E_ALL & ~E_DEPRECATED & ~E_USER_DEPRECATED);
ini_set('display_errors', '0');

require_once __DIR__ . '/../vendor/autoload.php';

/**
 * Run console interface
 */
try {
    $app = new BotCore();
    $app->run();
} catch (\Throwable $e) {
    print $e;
}
